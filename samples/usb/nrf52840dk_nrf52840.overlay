&usbd {
	midi_device {
		compatible = "usb-midi-device";

		midi_interface_1 {
			compatible = "usb-midi-interface";

			setting_1	{
				compatible = "usb-midi-setting";
				revision = "MIDI 1.0";

				jack_1: jack_1 {
					compatible = "usb-midi-in-jack";
					type = "EMBEDDED";
					id = <1>;
				};
		
				jack_2: jack_2 {
					compatible = "usb-midi-in-jack";
					type = "EXTERNAL";
					id = <2>;
				};
		
				jack_3: jack_3 {
					compatible = "usb-midi-out-jack";
					type = "EMBEDDED";
					input_pins = <1>;
					sources-pins = <1>;
					sources = <&jack_2>;
					id = <3>;
				};
		
				jack_4: jack_4 {
					compatible = "usb-midi-out-jack";
					type = "EXTERNAL";
					input_pins = <1>;
					sources = <&jack_1>;
					sources-pins = <1>;
					id = <4>;
				};
		
				endpoint_out_1 {
					compatible = "usb-midi-endpoint";
					direction = "OUT";
					type = "BULK";
					assoc-entities = <&jack_1>;
					
				};
		
				endpoint_in_1 {
					compatible = "usb-midi-endpoint";
					direction = "IN";
					type = "BULK";
					assoc-entities = <&jack_3>;
				};
			};
			
			setting_2 {
				compatible = "usb-midi-setting";
				revision = "MIDI 2.0";
			
				endpoint_out_2 {
					compatible = "usb-midi-endpoint";
					direction = "OUT";
					type = "BULK";
					assoc-entities = <&gt_1>;
					
				};
		
				endpoint_in_2 {
					compatible = "usb-midi-endpoint";
					direction = "IN";
					type = "INTERRUPT";
					assoc-entities = <&gt_1>;
				};
		
				gt_1: gt_1 {
					compatible = "usb-midi-group-terminal-block";
					id = <1>;
					type = "IN-OUT";
					group-terminal = <0>;
					num-group-terminals = <1>;
					protocol = "UNKNOWN";
					in-bandwidth = <1>;
				};
			};
		};
	};
};
